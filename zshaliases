alias ipa='ip a'
alias git='hub'
alias ports='netstat -tulpn'
alias sudo='sudo '
alias go-ipfs='cd-go github.com/ipfs/go-ipfs'
alias gff='git flow feature'
alias clr='clear'
alias hgrep='history | grep -i'
alias vimrc='vim ~/.vimrc'
alias xflux='xflux -l 51.1 -g 17'
alias cdtemp='cd $(mktemp -d)'
alias v='f -e vim'

alias gcf='git commit --fixup'
alias gcaf='git commit -a --fixup'
alias gcs='git commit --squash'
alias gcas='git commit -a --squash'
alias grbiom='git rebase -i origin/master'


